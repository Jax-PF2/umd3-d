usted level is 13+</li>\n</ul>\n</li>\n</ul>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[{"key":"FlatModifier","selector":"ac","value":1},{"key":"FlatModifier","selector":"spell-dc","value":1},{"key":"FlatModifier","selector":"spell-attack","value":1},{"key":"FlatModifier","selector":"saving-throw","value":1},{"key":"FlatModifier","selector":"skill-check","value":1},{"key":"FlatModifier","selector":"damage","value":2,"predicate":[{"nor":["arcane","divine","primal","occult"]}]},{"key":"FlatModifier","value":1,"selector":"attack"}],"slug":null,"schema":{"version":0.796,"lastMigration":{"datetime":"2022-11-06T22:35:36.937-05:00","version":{"schema":0.793,"foundry":"10.288","system":"4.3.4"}}},"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"_stats":{"systemId":"pf2e","systemVersion":"4.3.4","coreVersion":"10.288","createdTime":1666186707653,"modifiedTime":1667854532201,"lastModifiedBy":"aTWxsSrAlxp06IfB"},"folder":null,"sort":0,"_id":"G43Y5XXg1ObGZQyE"}
