ion":{"datetime":"2023-06-23T15:05:24.380-04:00","version":{"schema":0.769,"foundry":"10.291","system":"4.12.9"}}},"traits":{"value":["divine","necromancy"],"rarity":"common"},"actionType":{"value":"passive"},"category":null,"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"_stats":{"systemId":"pf2e","systemVersion":"4.12.9","coreVersion":"10.291","createdTime":1687547132619,"modifiedTime":1687702458366,"lastModifiedBy":"aTWxsSrAlxp06IfB"},"folder":null,"sort":0,"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"_id":"Gf9RhDVBPIm8lyzz"},{"name":"Nemhaith lvl 1: Site bound","type":"action","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/actions/Passive.webp","effects":[],"flags":{"core":{"sourceId":"Compendium.jam-and-jax-undead-templates.item.t2dFikzdYvFpuDzp"},"pf2e":{"grantedBy":{"id":"UeFPBBGcLnhqtCIW","onDelete":"cascade"}}},"system":{"description":{"gm":"","value":"<p><span id=\"docs-internal-guid-52242982-7fff-e868-9c31-5617eb70c6b3\">The nemhaith is bound to a place or large stationary object and can't venture more than 500 feet from this focus. </span>Destroying the ritual object or place used in the nemhaith's creation destroys the nemhaith, freeing its bound spirits to move on to the afterlife.</p>"},"source":{"value":""},"rules":[],"slug":null,"schema":{"version":0.84,"lastMigration":{"datetime":"2023-06-23T15:05:33.929-04:00","version":{"schema":0.769,"foundry":"10.291","system":"4.12.9"}}},"traits":{"value":[],"rarity":"common"},"actionType":{"value":"passive"},"category":null,"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"_stats":{"systemId":"pf2e","systemVersion":"4.12.9","coreVersion":"10.291","createdTime":1687547135764,"modifiedTime":1687702458367,"lastModifiedBy":"aTWxsSrAlxp06IfB"},"folder":null,"sort":0,"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"_id":"eua18Kbr4iMUpjwK"},{"name":"Nemhaith lvl 7: Rage of Spirits","type":"action","i