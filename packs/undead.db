lockquote>"},"source":{"value":""},"rules":[{"key":"GrantItem","uuid":"Compendium.jam-and-jax-undead-templates.item.m3C7ydIOBns5BOfE","ReplaceSelf":false,"allowDuplicate":false,"flag":"mummyLvl1CorporealUndeadTraits"},{"key":"GrantItem","uuid":"Compendium.jam-and-jax-undead-templates.item.lTlq4DaGMzoPjGaJ","ReplaceSelf":false,"allowDuplicate":false,"flag":"mummyLvl1MummysRags"},{"key":"GrantItem","uuid":"Compendium.jam-and-jax-undead-templates.item.yK1IOD9mkZlVh2Yw","ReplaceSelf":false,"predicate":[{"gt":["self:level",6]}],"allowDuplicate":false,"reevaluateOnUpdate":true,"flag":"mummyLvl7AuraOfDespair"},{"key":"GrantItem","uuid":"Compendium.jam-and-jax-undead-templates.item.NRyxXBDJx4agYca4","ReplaceSelf":false,"predicate":[{"gt":["self:level",12]}],"allowDuplicate":false,"reevaluateOnUpdate":true,"flag":"mummyLvl13InsidiousMummyRot"}],"slug":null,"schema":{"version":0.84,"lastMigration":null},"traits":{"value":[]},"actionType":{"value":"passive"},"category":null,"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"img":"systems/pf2e/icons/effects/treat-poison.webp","ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Item.YzBhKYKirsgRX8XH"},"pf2e":{"itemGrants":{"mummyLvl1CorporealUndeadTraits":{"id":"Rma2T7gP3a03BBBj","onDelete":"detach"},"mummyLvl1MummysRags":{"id":"e7ECk13KOQ6Rr20F","onDelete":"detach"},"mummyLvl7AuraOfDespair":{"id":"NKozCbwFt1YuOc72","onDelete":"detach"},"mummyLvl13InsidiousMummyRot":{"id":"1EvUQ1xMoONgcwN4","onDelete":"detach"}}}},"_stats":{"systemId":"pf2e","systemVersion":"4.12.9","coreVersion":"10.291","createdTime":1687547507988,"modifiedTime":1687702431013,"lastModifiedBy":"aTWxsSrAlxp06IfB"},"folder":null,"sort":0,"_id":"SAyQHog9gJ056fJY"},{"name":"Mummy lvl 1 : Corporeal Undead Traits","type":"action","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/actions/Passive.webp","effects":[],"flags":{"scene-packer":{"hash":"47467241a7d0192c27711fafc51e464525c35880","sourceId":"Item.r8OxWTjkjQ9A1EaU"},"core":{"sourceId":"Compendium.jam-and-jax-undead-templates.item.m3C7ydIOBns5BOfE"},"pf2e":{"grantedBy":{"id":"SAyQHog9gJ056fJY","onDelete":"cascade"}}},"system":{"description":{"gm":"","value":"<p dir=\"ltr\">Corporeal Undead Traits</p>\n<ul>\n<li dir=\"ltr\">Speed: 25 feet</li>\n<li dir=\"ltr\">Size: Medium</li>\n<li dir=\"ltr\">Languages: The creature knows the Necril language, and Common as the GM sees fit. (<strong>Manually type in)</strong></li>\n<li dir=\"ltr\">Undead Vision: Darkvision <strong>(Manually type in)</strong></li>\n<li dir=\"ltr\">Negative Healing: A creature with negative healing draws health from negative energy rather than positive energy. It is damaged by positive damage and is not healed by positive healing effects. It does not take negative damage, and it is healed by negative effects that heal undead.</li>\n<li><span id=\"docs-internal-guid-62614a9c-7fff-a1a8-841e-f28ab476529c\">Undead Immunities: Death effects, disease, paralyzed, poison, unconscious.</span></li>\n</ul>"},"source":{"value":""},"rules":[{"key":"Immunity","type":"death-effects"},{"key":"Immunity","type":"disease"},{"key":"Immunity","type":"paralyzed"},{"key":"Immunity","type":"poison"},{"key":"Immunity","type":"unconscious"},{"key":"BaseSpeed","selector":"land-speed","value":25},{"key":"ActorTraits","add":["undead"]}],"slug":null,"schema":{"version":0.84,"lastMigration":{"datetime":"2023-06-23T15:05:33.672-04:00","version":{"schema":0.769,"foundry":"10.291","system":"4.12.9"}}},"traits":{"value":[],"rarity":"common"},"actionType":{"value":"passive"},"category":null,"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"_stats":{"systemId":"pf2e","systemVersion":"4.12.9","coreVersion":"10.291","createdTime":1687547135759,"modifiedTime":1687702431015,"lastModifiedBy":"aTWxsSrAlxp06IfB"},"folder":null,"sort":0,"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"_id":"Rma2T7gP3a03BBBj"},{"name":"Mum